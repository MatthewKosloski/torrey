{
	"branches": [
		"issues-34-ci-pipeline"
	],
	"tagFormat": "${version}",
	"plugins": [
		"@semantic-release/commit-analyzer",
		"@semantic-release/release-notes-generator",
		["@semantic-release/github", {
		"assets": [
			{"path": "./target/-torreyc-${nextRelease.version}.jar", "label": "compiler"}
		]
		}],
		["@semantic-release/exec", {
			"prepareCmd": "mvn versions:set -DgenerateBackupPoms=false -DnewVersion=${nextRelease.version}"
		}]
	],
	"dryRun": false
}